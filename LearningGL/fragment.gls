#version 460 core

out vec4 fragmentColor;
in vec4 vertexColor;
in vec2 textureCoord;

uniform sampler2D background;
uniform sampler2D face;

void main() {
	//fragmentColor = texture(background, textureCoord);
	fragmentColor = mix(texture(background, textureCoord), texture(face, textureCoord), 0.2);
}